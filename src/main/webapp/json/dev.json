          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "email",
            "in": "formData",
            "description": "邮箱",
            "required": true,
            "type": "string",
            "format": ""
          },
          {
            "name": "nickName",
